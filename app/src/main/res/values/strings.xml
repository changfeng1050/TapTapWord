    <string name="word_ninja_name">Ninja</string>
    <string name="word_ninja_ph_am">"'nɪndʒə"</string>
    <string name="word_ninja_ph_en">"'nɪndʒə"</string>
    <string name="word_ninja_means">n.忍者（受过日本传统打斗和轻功训练的人）\nv.请将忍者监听打开，以便忍者在你阅读的时候窃取单词</string>

    <string name="word_recent_name">recent</string>
    <string name="word_recent_ph_am">"'risənt"</string>
    <string name="word_recent_ph_en">"'riːs(ə)nt"</string>
    <string name="word_recent_means">adj.最近的；新近的;\nadj.这里存放单词忍者窃取的单词</string>

    <string name="word_word_name">word</string>
    <string name="word_word_ph_am">"wɜrd"</string>
    <string name="word_word_ph_en">"wɜː(r)d"</string>
    <string name="word_word_means">n.词；单词；字；消息\nv.措辞；用词\nint.说得对 \nv.单词忍者窃取的单词都会保存在这里，你可以对长按单词卡片进行删除或者归档</string>

    <string name="word_archive_name">archive</string>
    <string name="word_archive_ph_am">"'ɑr.kaɪv"</string>
    <string name="word_archive_ph_en">"'ɑː(r)kaɪv"</string>
    <string name="word_archive_means">n.档案；档案馆；档案室\nv.把…存档；把…归档；将（不常用消息）存档\nv.长按单词卡片进行归档</string>

    <string name="word_unarchive_name">unarchive</string>
    <string name="word_unarchive_ph_am">"ʌn'ɑː(r)kaɪv"</string>
    <string name="word_unarchive_ph_en">"ʌn'ɑː(r)kaɪv"</string>
    <string name="word_unarchive_means">v.取消存档；反归档\nv.长按单词卡片，然后按右上角“取消归档”，可将单词移到最近单词</string>

    <string name="word_delete2_name">delete</string>
    <string name="word_delete2_ph_am">"dɪ'lit"</string>
    <string name="word_delete2_ph_en">"dɪ'liːt"</string>
    <string name="word_delete2_means">v.删去；删除\nv.长按单词卡片进行删除</string>

    <string name="word_delete_name">delete</string>
    <string name="word_delete_ph_am">"dɪ'lit"</string>
